{
  "name": "BetterMediaKeys", 
  "version": "1.0.0.1",
  "description": "A chromium extension that changes the handling of media keys.",
  "action": {},
  "manifest_version": 3,
   "content_scripts": [
    {
    "run_at": "document_start",
    "matches": ["https://*.youtube.com/*"],
    "js": ["mediaKeys.js"]
    }
   ],
   "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}